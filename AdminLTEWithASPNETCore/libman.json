{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "Chart.js@2.9.4",
      "destination": "wwwroot/lib/Chart.js/"
    },
    {
      "library": "jqueryui@1.12.1",
      "destination": "wwwroot/lib/jqueryui/"
    },
    {
      "library": "noty@3.1.4",
      "destination": "wwwroot/noty/"
    },
    {
      "library": "toastr.js@2.1.4",
      "destination": "wwwroot/lib/toastr.js/"
    },
    {
      "provider": "unpkg",
      "library": "@microsoft/signalr@latest",
      "destination": "wwwroot/lib/signalr/",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.min.js",
        "dist/browser/signalr.js.map",
        "dist/browser/signalr.min.js.map"
      ]
    }
  ]
}